<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.michelapplication.moodtracker.MainActivity">

    <android.support.v4.view.ViewPager
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/viewPager">

    </android.support.v4.view.ViewPager>

    <Button
        android:id="@+id/button_commenter"
        android:layout_width="32dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="11dp"
        android:background="@drawable/ic_note_add_black"/>

    <Button
        android:id="@+id/button_history_black"
        android:layout_width="42dp"
        android:layout_height="36dp"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="11dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/ic_history_black"/>


</RelativeLayout>
